{
  "name": "directory-recreate",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": " https://github.com/dsb-norge/directory-recreate",
    "directory": "src"
  },
  "scripts": {
    "package": "npx ncc build src/index.ts -m -o dist --license licenses.txt",
    "lint": "npx eslint . --ext .ts",
    "lint:fix": "npx eslint . --ext .ts --fix",
    "test": "npx jest",
    "prepare": "husky",
    "all": "npm run lint && npm run test && npm run package"
  },
  "keywords": [],
  "author": "Chris Aurora Neumann Ruud",
  "contributors": [
    "Peder Schmedling"
  ],
  "license": "ISC",
  "description": "This action removes and re-creates a directory including all files and sub-directories within.",
  "jest": {
    "preset": "ts-jest",
    "verbose": true,
    "clearMocks": true,
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "dependencies": {
    "@actions/core": "1.11.1",
    "@actions/github": "6.0.0"
  },
  "devDependencies": {
    "@dsb-norge/eslint-config-dsb-vue3-ts": "2.2.0",
    "@rushstack/eslint-patch": "1.10.4",
    "@types/jest": "29.5.13",
    "@types/node": "22.7.6",
    "@vercel/ncc": "0.38.2",
    "eslint": "8.57.1",
    "husky": "9.1.6",
    "jest": "29.7.0",
    "ts-jest": "29.2.5",
    "typescript": "5.6.3"
  }
}
